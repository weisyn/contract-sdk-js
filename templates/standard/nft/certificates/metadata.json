{
  "id": "certificates",
  "name": "Ucertificates NFT",
  "category": "NFT",
  "description": "Ucertificates NFT合约，实现相关NFT的代币化",
  "tags": [
    "standard",
    "nft",
    "certificates"
  ],
  "parameters": [],
  "risks": [],
  "prerequisites": [
    "了解 TypeScript/AssemblyScript 基础语法",
    "了解 WES 合约基本概念",
    "了解 NFT 的基本概念"
  ],
  "examples": [
    "wes contract deploy contract.wasm --function Initialize",
    "wes contract call <contract_address> --function MintNFT --params '{\"to\":\"...\",\"token_id\":\"certificates_001\"}'"
  ],
  "codePreview": "const result = NFT.mint(to, tokenID, metadata);",
  "sourcePath": "contract.ts",
  "version": "1.0.0",
  "author": "WES Contract SDK Team",
  "license": "MIT",
  "language": "typescript",
  "level": "standard",
  "helpers": [
    "nft"
  ],
  "methods": [
    {
      "name": "Initialize",
      "type": "write",
      "parameters": [],
      "returnType": "number",
      "description": "初始化合约",
      "isReferenceOnly": false
    },
    {
      "name": "MintNFT",
      "type": "write",
      "parameters": [
        {
          "name": "to",
          "type": "address",
          "required": true,
          "description": "接收者地址"
        },
        {
          "name": "token_id",
          "type": "string",
          "required": true,
          "description": "证书唯一标识"
        },
        {
          "name": "certificate_name",
          "type": "string",
          "required": true,
          "description": "证书名称"
        },
        {
          "name": "issuer",
          "type": "string",
          "required": true,
          "description": "颁发机构"
        }
      ],
      "returnType": "number",
      "description": "铸造唯一的证书凭证NFT",
      "isReferenceOnly": false
    },
    {
      "name": "TransferNFT",
      "type": "write",
      "parameters": [
        {
          "name": "to",
          "type": "address",
          "required": true,
          "description": "接收者地址"
        },
        {
          "name": "token_id",
          "type": "string",
          "required": true,
          "description": "证书唯一标识"
        }
      ],
      "returnType": "number",
      "description": "转移证书所有权",
      "isReferenceOnly": false
    },
    {
      "name": "QueryNFT",
      "type": "read",
      "parameters": [
        {
          "name": "token_id",
          "type": "string",
          "required": true,
          "description": "证书唯一标识"
        }
      ],
      "returnType": "string",
      "description": "查询证书信息和所有者",
      "isReferenceOnly": true
    }
  ],
  "sdkCompatibility": {
    "js": ">=0.1.0-alpha <0.2.0"
  },
  "sinceSdk": {
    "js": "0.1.0-alpha"
  }
}
